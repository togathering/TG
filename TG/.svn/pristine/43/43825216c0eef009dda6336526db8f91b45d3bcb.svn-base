package com.tg.common.beans;

public class FriendBean {
	private String id;
	private String fid;
	
	public FriendBean() {
		
	}
	
	public FriendBean(String id, String fid) {
		this.id = id;
		this.fid = fid;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getFid() {
		return fid;
	}

	public void setFid(String fid) {
		this.fid = fid;
	}
	
	
}
